{
  "name": "lalaz/reactive",
  "description": "Interactive UI components with server-side state - no JavaScript required.",
  "type": "library",
  "keywords": [
    "lalaz",
    "reactive",
    "components",
    "livewire",
    "alpine",
    "interactive"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Gregory Serrao",
      "email": "hello@lalaz.dev"
    }
  ],
  "require": {
    "php": "^8.3",
    "lalaz/framework": "^1.0.0-rc.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0"
  },
  "autoload": {
    "psr-4": {
      "Lalaz\\Reactive\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Lalaz\\Reactive\\Tests\\": "tests/"
    }
  },
  "extra": {
    "lalaz": {
      "providers": [
        "Lalaz\\Reactive\\ReactiveServiceProvider"
      ]
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
